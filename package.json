{
  "name": "app",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "patch-package",
    "typecheck:e2e": "tsc -p test/e2e/tsconfig.json --noEmit",
    "typecheck:source": "tsc -p tsconfig.json --noEmit",
    "typecheck": "yarn typecheck:source && yarn typecheck:e2e",
    "android": "react-native run-android",
    "start": "react-native start",
    "lint:source": "ESLINT_TARGET=SourceCode eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:e2e": "ESLINT_TARGET=IntegrationTests eslint 'test/e2e/**/*.{ts,tsx,js,jsx}'",
    "lint": "yarn lint:source && yarn lint:e2e",
    "fix": "yarn lint:source --fix && yarn lint:e2e --fix",
    "e2e": "jest --projects e2e",
    "apk:debug": "mkdir -p ./android/app/src/main/assets && react-native bundle --dev false --platform android --entry-file src/App.tsx --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "apk:release": "react-native run-android --variant=release"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.15",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@typescript-eslint/typescript-estree": "^5.10.1",
    "@wdio/cli": "^7.16.14",
    "@wdio/devtools-service": "^7.16.14",
    "appium": "^1.22.2",
    "babel-jest": "^27.4.6",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.4.7",
    "jest-environment-webdriverio": "^1.0.13",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "request": "^2.88.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "webdriverio": "^7.16.14"
  }
}
