{
  "name": "ReactNativePrototype",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --projects .",
    "test:e2e": "jest --projects e2e",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "node scripts/postinstall__typescript-eslint-language-server.js",
    "apk:debug": "mkdir -p ./android/app/src/main/assets && react-native bundle --dev false --platform android --entry-file src/App.tsx --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res && cd android && ./gradlew assembleDebug"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.1",
    "@types/react-native": "^0.60.26",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "@typescript-eslint/typescript-estree": "^2.13.0",
    "@wdio/appium-service": "^5.16.11",
    "@wdio/cli": "^5.18.1",
    "appium": "^1.16.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^24.9.0",
    "jest-environment-webdriverio": "^1.0.13",
    "metro-react-native-babel-preset": "^0.57.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.12.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "typescript-eslint-language-service": "https://github.com/manuth/typescript-eslint-language-service/tarball/eslint-6.7-fix",
    "webdriverio": "^5.18.0"
  },
  "jest": {
    "preset": "react-native",
    "projects": [
      "<rootDir>/jest.config.js",
      "<rootDir>/e2e/jest.config.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
