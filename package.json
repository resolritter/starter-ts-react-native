{
  "name": "app",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "patch-package",
    "typecheck:e2e": "tsc --noEmit -p test/e2e/tsconfig.json",
    "typecheck:source": "tsc --noEmit -p tsconfig.json",
    "typecheck": "yarn typecheck:source && yarn typecheck:e2e",
    "android": "react-native run-android",
    "start": "react-native start",
    "prefix:lint:source": "ESLINT_TARGET=SourceCode eslint",
    "lint:source": "yarn prefix:lint:source '*.{ts,tsx,js,jsx}' 'src/**/*.{ts,tsx,js,jsx}'",
    "prefix:lint:e2e": "ESLINT_TARGET=IntegrationTests eslint",
    "lint:e2e": "yarn prefix:lint:e2e 'test/e2e/**/*.{ts,tsx,js,jsx}'",
    "lint": "yarn lint:source && yarn lint:e2e",
    "prettier:fix": "prettier --write '**/*.{html,json}'",
    "fix": "yarn lint:source --fix && yarn lint:e2e --fix && yarn prettier:fix",
    "e2e": "jest --projects e2e",
    "apk:debug": "mkdir -p ./android/app/src/main/assets && react-native bundle --dev false --platform android --entry-file src/App.tsx --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "apk:release": "react-native run-android --variant=release",
    "staged": "scripts/staged"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.7.2",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.11.0",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.16",
    "@types/react-native": "0.66.15",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "@typescript-eslint/typescript-estree": "5.11.0",
    "@wdio/cli": "7.16.14",
    "@wdio/devtools-service": "7.16.14",
    "appium": "1.22.2",
    "babel-jest": "27.5.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "27.5.1",
    "jest-environment-webdriverio": "1.0.13",
    "metro-react-native-babel-preset": "0.68.0",
    "patch-package": "6.4.7",
    "prettier": "2.5.1",
    "request": "2.88.0",
    "ts-node": "10.5.0",
    "tsc-files": "^1.1.3",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5",
    "webdriverio": "7.16.14"
  }
}
